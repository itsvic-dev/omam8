; write 1 to all IO pins

loop:
    ; a - target pin
    ; b - whatever else

    mov %a, %b ; load pin into B
    shl 2, %b ; shift it to the left by 2 bits
    add 2, %b ; add 0b10 to the end of b
    sio %b ; set pin mode

    mov 1, %b ; load 1 into B
    wio %b, %a ; write %b into pin %a

    add 1, %a ; increment pin counter
    jmp .loop
